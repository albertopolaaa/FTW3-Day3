   "source": [
    "def is_palindrome(x):\n",
    "    if len(x) <= 1:\n",
    "        return True\n",
    "    elif x[0] != x[-1]:\n",
    "        return False\n",
    "    else:\n",
    "        return is_palindrome(x[1:-1])\n",
    "    \n",
    "print(is_palindrome(['hello', 'hi', 'hello']))\n",
    "is_palindrome('dad')\n",
    "is_palindrome('12321')"
   ]
